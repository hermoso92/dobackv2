# Configuración de API para DobackSoft
# Copia este archivo como .env.local y ajusta los valores según tu entorno

# URL base de la API backend
REACT_APP_API_BASE_URL=http://localhost:9998

# Timeout para requests de API (en milisegundos)
REACT_APP_API_TIMEOUT=30000

# Configuración de desarrollo
NODE_ENV=development

# Configuración de logging
REACT_APP_LOG_LEVEL=info

# Configuración de mapas (si se usa)
REACT_APP_MAP_API_KEY=your_map_api_key_here

# Configuración de Sentry (si se usa)
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
